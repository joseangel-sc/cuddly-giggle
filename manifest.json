{
  "name": "GEXMAIL_V0.0.1",
  "description": "GEXMAIL (https://gmail.com/) scrap",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://gmail.com/*"
      ],
      "js": ["index.js"],
      "run_at": "document_end"
    }],
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://api.justdecision.com/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon.png"
    }
  },
  "icons": {
    "16": "images/icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://api.openai.com; connect-src 'self' https://api.justdecision.com;"
  }
}
